{
  "mappings": {
      "ID": "id",
      "FirstName": "first_name",
      "LastName": "last_name",
      "DateOfBirth": "dob"
  },
  "transformations": {
      "name": {
          "type": "concat",
          "params": {
              "fields": [
                  "FirstName",
                  "LastName"
              ],
              "extras": {
                  "separator": " "
              }
          }
      },
      "age": {
          "type": "calculate",
          "params": {
              "fields": [
                  "DateOfBirth",
                  "CurrentTime"
              ],
              "extras": {
                  "operation": "time_difference",
                  "format": "2006-01-02",
                  "unit": "years",
                  "adjust_for_birthday": true
              }
          }
      }
  }
}
